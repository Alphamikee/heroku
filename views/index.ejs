<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <a href="new">new message</a>
    <h1><%= title %></h1>
      <% febChar.forEach(function( eachPerson){ %>
        <div style="display: inline-block;margin: 10px;background-color: lightgray;padding: 3px;text-align: center;">
        <h3> <%= eachPerson.user %></h3>
        <h4> <%= eachPerson.text%> </h4>
        <h5><%= eachPerson.added%></h5>
      </div>
    <% }) %>
  </body>
</html>
